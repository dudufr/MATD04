#include <stdio.h>

struct node {
  int data;
  struct node* next;
  };
  
int IsEmpty (struct node* elemento){
  if (elemento != NULL)
    return 0;
  else
    return 1;
};

int size(struct node* elemento){
	int count = 0;
	while (elemento->next != NULL){
		count = count + 1;
	};
	return count;
};

int main(void) {

  struct node* head = NULL;
  printf ("%d", IsEmpty(head));
  
  return 0;
}
