#include <stdio.h>

int main(void) {
  int n, vet[50], i, j, aux, res;

  scanf("%d", &n);

  for (i=0; i<n; i++){
    scanf("%d", &vet[i]);
    printf("vet[%d] = %d\n", i, vet[i]);
  }

  i=0;

  for (i=0; i<n-1; i++){
    for (j=i+1; j<n; j++){
      if (vet[i]>vet[j]){
        aux=vet[i];
        vet[i]=vet[j];
        vet[j]=aux;
      }
    }
  }

  i=0;

  scanf("%d", &res);

  for (i=0; i<n; i++){
    if(vet[i]==res){
      printf("%d", &i);
      break;
    }
  }
  
  return 0;
}
